  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:integration": "dotenv -e .env.test -- vitest run --config vitest.config.mts",
    "test:coverage": "dotenv -e .env.test -- vitest run --config vitest.config.mts --coverage",
    "test:db:up": "docker compose -f docker-compose.test.yml up -d --wait",
    "test:db:down": "docker compose -f docker-compose.test.yml down -v",
